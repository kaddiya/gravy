<web-app xmlns='http://java.sun.com/xml/ns/javaee' xsi:schemaLocation='http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' version='3.0'>
    <servlet>
        <servlet-name>
            restling
        </servlet-name>
        <servlet-class>
            restling.RestlingServlet
        </servlet-class>
        <init-param>
            <param-name>
                guice-module
            </param-name>
            <param-value>
                com.ServiceApiModule
            </param-value>
        </init-param>
        <load-on-startup>
            1
        </load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>
            restling
        </servlet-name>
        <url-pattern>
            /*
        </url-pattern>
    </servlet-mapping>
</web-app>
